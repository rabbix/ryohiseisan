<tr id='<%= dom_id employee %>'>
  <td><%= employee.name %></td>
  <% unless current_user.expired?( @now ) -%>
    <td><%= link_to t('button.edit'), edit_company_employee_path(@company, employee), remote: true, class: 'btn btn-warning' %>
    <%= link_to t('button.destroy'), company_employee_path(@company, employee), method: :delete, confirm: t('Are you sure?'), remote: true, class: 'btn btn-danger' %></td>
  <% end -%>
</tr>
